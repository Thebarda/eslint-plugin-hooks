{
  "name": "eslint-plugin-hooks-sort",
  "version": "0.1.0-alpha.1",
  "description": "A simple organizer for ordering hooks.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "main": "dist/index.js",
  "author": {
    "name": "Romullo @hiukky",
    "email": "developermarsh@gmail.com",
    "url": "https://hiukky.com/"
  },
  "scripts": {
    "clean": "rimraf dist typings",
    "prebuild": "yarn clean",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.spec.ts' --recursive",
    "build": "tsc -d -p .",
    "format": "prettier --write \"**/*.ts\""
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "requireindex": "~1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/chai": "^4.2.12",
    "@types/eslint": "^7.2.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.43",
    "@types/requireindex": "^1.2.0",
    "eslint": "^7.5.0",
    "husky": "^4.2.5",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
